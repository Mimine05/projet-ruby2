<p id="notice"><%= notice %></p>

<h1>Ftus</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ftus.each do |ftu| %>
      <tr>
        <td><%= link_to 'Show', ftu %></td>
        <td><%= link_to 'Edit', edit_ftu_path(ftu) %></td>
        <td><%= link_to 'Destroy', ftu, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ftu', new_ftu_path %>
